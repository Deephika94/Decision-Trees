    "n_estimates = [10,20,50,100]\n",
    "train_accuracy = []\n",
    "test_accuracy = []\n",
    "\n",
    "for n in n_estimates:\n",
    "    rf_obj = RandomForestClassifier(n_estimators=n,criterion=\"entropy\",random_state=100)\n",
    "    rf_obj.fit(train_x,train_y)\n",
    "    test_score = accuracy_score(test_y,rf_obj.predict(test_x))\n",
    "    train_score = accuracy_score(train_y,rf_obj.predict(train_x))\n",
    "    train_accuracy.append(train_score)\n",
    "    test_accuracy.append(test_score)\n",
    "\n",
    "plt.plot(train_accuracy)\n",
    "plt.plot(test_accuracy)\n",
    "plt.xticks(ticks=range(len(n_estimates)), labels = n_estimates)\n",
    "plt.title(\"n-estimator tuning\")\n",
    "plt.ylabel(\"accuracy scores\")\n",
    "plt.legend([\"Train\",\"Test\"])"
   ]
